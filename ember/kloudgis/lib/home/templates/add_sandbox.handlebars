{{#view id="add-sandbox-title" class="page-title"}}
	{{KG.homePanelController.addTitle}}
{{/view}}
{{#view id="inner-add-sandbox-panel"}}
	<div>
		{{view KG.TextField placeholder_not_loc="_sandboxName" valueBinding="KG.addSandboxController.name"}}	
	</div>
	{{loc _position class="field-label"}}
	<div id="add-sandbox-map"></div>
	{{#view KG.Button class="white-button" sc_action="cancelCreateAction" titleBinding="KG.addSandboxController.cancelCreateTooltip"}}
		<img src="resources/images/cancel_30.png">
	{{/view}}
	{{#view KG.Button class="white-button" sc_action="commitCreateAction" titleBinding="KG.addSandboxController.commitCreateTooltip"}}
		<img src="resources/images/checkmark_30.png">
	{{/view}}
{{/view}}